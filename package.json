{
  "name": "multi-tenants-webpack-plugin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "workspaces": [
    "example/*"
  ],
  "dependencies": {
    "css-loader": "^6.7.3",
    "mini-svg-data-uri": "1.4.4",
    "postcss": "8.4.21",
    "postcss-loader": "7.0.2",
    "postcss-modules-extract-imports": "3.0.0",
    "postcss-modules-local-by-default": "4.0.0",
    "postcss-modules-scope": "3.0.0",
    "postcss-url": "10.1.3",
    "sass-loader": "^13.2.0",
    "webpack-sources": "^3.2.3"
  },
  "devDependencies": {
    "webpack": "5.76.1"
  },
  "scripts": {
    "dev": "yarn workspace @example/app dev",
    "debug": "yarn workspace @example/app debug",
    "serve": "yarn workspace @example/app http-server ./dist",
    "start": "yarn dev && yarn serve"
  }
}
